{
  "name": "@seinopsys-forks/openapi-to-typescript",
  "version": "0.0.2",
  "description": "Generate TypeScript typings based on an OpenAPI schema object.",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js"
  ],
  "repository": {
    "url": "https://github.com/SeinopSys/openapi-to-typescript"
  },
  "author": "László Szűcs <ifrozen@gmail.com>",
  "maintainers": [
    {
      "name": "SeinopSys",
      "email": "david@seinopsys.dev",
      "url": "https://seinopsys.dev"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "cli": "ts-node ./lib/cli",
    "lint": "eslint '**/*.ts'",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "json-schema-ref-parser": "^9.0.6",
    "json-schema-to-typescript": "^9.1.1",
    "lodash": "^4.17.20",
    "source-map-support": "^0.5.19",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.9",
    "@types/yargs": "^15.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "execa": "^4.0.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
}
