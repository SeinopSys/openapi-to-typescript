{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Swagger Nullables",
    "license": {
      "name": "MIT"
    }
  },
  "servers": [
    {
      "url": "https://example.com"
    }
  ],
  "paths": {
    "/list": {
      "get": {
        "operationId": "GetList",
        "responses": {
          "200": {
            "description": "Image data",
            "content": {
              "application/json": {
                "schema": {
                  "description": "Random values",
                  "type": "object",
                  "additionalProperties": false,
                  "required": [
                    "a",
                    "d"
                  ],
                  "properties": {
                    "a": {
                      "type": "number"
                    },
                    "b": {
                      "type": "boolean"
                    },
                    "c": {
                      "nullable": true,
                      "type": "string",
                      "enum": [
                        "c1",
                        "c2"
                      ]
                    },
                    "d": {
                      "nullable": true,
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "security": []
      }
    }
  },
  "components": {
    "schemas": {
      "SomeOtherType": {
        "type": "string",
        "enum": [
          "45",
          "90"
        ]
      },
      "AndAnotherOne": {
        "type": "number",
        "enum": [
          30,
          60,
          90
        ]
      },
      "MoreNullables": {
        "description": "More random values",
        "type": "object",
        "additionalProperties": false,
        "required": [
          "f"
        ],
        "properties": {
          "e": {
            "nullable": true,
            "oneOf": [
              {
                "$ref": "#/components/schemas/SomeOtherType"
              },
              {
                "$ref": "#/components/schemas/AndAnotherOne"
              }
            ]
          },
          "f": {
            "nullable": true,
            "allOf": [
              {
                "$ref": "#/components/schemas/SomeOtherType"
              },
              {
                "$ref": "#/components/schemas/AndAnotherOne"
              }
            ]
          }
        }
      }
    }
  }
}
